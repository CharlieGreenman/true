theme: 'herman'
dest: gh-pages/sassdoc

autofill:
  - throw
  - content
  - require

groups:
  assert: Assertion API
  test: Testing API
  report: Reporting API
  private-assert: '[assertion helpers]'
  private-context: '[data helpers]'
  private-message: '[reporting helpers]'
  private-test: '[module helpers]'
