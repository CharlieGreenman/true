theme: 'herman'
dest: gh-pages/sassdoc

autofill:
  - throw
  - content
  - require

groups:
  api-assert: Assertion API
  api-test: Testing API
  api-report: Reporting API
  private-assert: '[assertion helpers]'
  private-context: '[data helpers]'
  private-message: '[reporting helpers]'
  private-testing: '[test helpers]'
  private-utils: [internal utilities]
